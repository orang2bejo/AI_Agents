# Konfigurasi API Keys untuk Jarvis AI
# Uncomment dan isi dengan API key yang valid jika diperlukan

# Google AI API Key (opsional)
# GOOGLE_API_KEY='your_google_api_key_here'

# OpenAI API Key (opsional)
# OPENAI_API_KEY='your_openai_api_key_here'

# Anthropic API Key (opsional)
# ANTHROPIC_API_KEY='your_anthropic_api_key_here'

# Mode pengembangan - nonaktifkan layanan eksternal
DEVELOPMENT_MODE=true
DISABLE_EXTERNAL_APIS=true

# Logging level
LOG_LEVEL=INFO